    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
    <_PropertySheetDisplayName>gtkglextmm 1.2 Debug</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
